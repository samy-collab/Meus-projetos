                                                  escreva("mmmmMMmmmmmmmmMMMM++--++##################################mm########################################@@########################mmmmMM++--MMMM\n")
                                                  escreva("MMmm--mmMM@@mmMM++..@@####################################@@######################################@@@@@@######################@@::--@@MM@@++\n")
                                                  escreva("mm..  ::MMMMMM++::########################################mm######################################MMMMMM########################--MM@@@@--..\n")
                                                  escreva("mm    ++mmMM@@++##########################################mm######################################MM@@@@########################::@@@@mm::--\n")
                                                  escreva("--    mmmmMM--##########################@@##@@########@@##MM######################################mm@@@@########################::MMMM..::mm\n")
                                                  escreva("..--  MMMMmmMM################@@@@MMMM--++mm@@############MM######################################@@@@@@########################mm@@----++MM\n")
                                                  escreva("::  ++mmmm::##################MM@@MM++MM::mmMMMM@@######@@########################################MMMM@@##########################::..--MM@@\n")
                                                  escreva("::..mmMM++--##################MMMM++MM##++++MMMMMMMMMM@@##########################################@@MMMM@@########################mm++MMMM@@\n")
                                                  escreva("..--++MM  mm############@@MM##MM##@@MM##@@####mm..mmMMMMmm########################################@@@@MM@@##########################mm++##@@\n")
                                                  escreva("  ++++mm::@@##########@@@@@@MM@@############@@##@@@@--@@@@##########################################@@##@@##########################++@@@@##\n")
                                                  escreva("  ++mm--::MM##########@@MMmm########@@##########@@MM####mmmm@@MM####################################MM@@@@##########################MM@@@@@@\n")
                                                  escreva("++++++  ..mm########@@MM@@####@@####################mm##MMmmmm##@@MMMMMMMMMMMMMMmm++MM@@############MM@@@@############################@@MMmm\n")
