                                                escreva("                                        ####      ++++++++++MM##          @@##++++++++++++++++@@##@@                                       \n")
                                                escreva("                                      ####      ++++++++++++++##          @@##++++++++++++++++++####                                       \n")
                                                escreva("                                    mm##@@      ++++++++++++++##          ##mm++++++++++++++++++mm####                                     \n")
                                                escreva("                                    ####      ++++++++++++++++##--        ##++++++++++++++++++++++####--                                   \n")
                                                escreva("                                  @@##::    --++++++++++++++++##--        ##++++++++++++++++++++++mm####                                   \n")
                                                escreva("                                  ####      ++++++++++++++++++@@@@      ..##++++++++++++++++++++++++@@##@@                                 \n")
                                                escreva("                                ####..    ::++++++++++++++++++MM##      mm##++++++++++++++++++++++++++####                                 \n")
                                                escreva("                              --####      ++++++++++++++++++++mm##      mm##++++++++++++++++++++++++++MM##@@                               \n")
                                                escreva("                              ####      ++++++++++++++++++++++++##      ##MM++++++++++++++++++++++++++++####..                             \n")
                                                escreva("                            @@##MM      ++++++++++++++++++++++++##      ##mm++++++++++++++++++++++++++++++####                             \n")
                                                escreva("                            ####      ++++++++++++++++++++++++++##      ##mm++++++++++++++++++++++++++++++####::                           \n")
                                                escreva("                          @@##++    --++++++++++++++++++++++++++##++    ##++++++++++++++++++++++++++++++++++####                           \n")
                                                escreva("                        ..####      ++++++++++++++++++++++++++++####  --##++++++++++++++++++++++++++++++++++@@##MM                         \n")
                                                escreva("                        ####      ::++++++++++++++++++++++++++++@@##  ::##++++++++++++++++++++++++++++++++++++####                         \n")
                                                escreva("                      --####      ++++++++++++++++++++++++++++++mm##  @@##++++++++++++++++++++++++++++++++++++MM####                       \n")
                                                escreva("                      ####      ++++++++++++++++++++++++++++++++++##  ##MM++++++++++++++++++++++++++++++++++++++####                       \n")
                                                escreva("                    MM##mm    ..++++++++++++++++++++++++++++++++++######++++++++++++++++++++++++++++++++++++++++++####                     \n")
                                                escreva("                    ####      ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++####++                   \n")
                                                escreva("                  ####::    ::++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++####                   \n")
                                                escreva("                ..####      ++++++++++++++++++++++++++++++++++++@@########++++++++++++++++++++++++++++++++++++++++++@@##@@                 \n")
                                                escreva("                ####      ::++++++++++++++++++++++++++++++++++@@##      ####++++++++++++++++++++++++++++++++++++++++++####                 \n")
                                                escreva("              ::####      ++++++++++++++++++++++++++++++++++++##--        ##++++++++++++++++++++++++++++++++++++++++++MM####               \n")
                                                escreva("              ####      ++++++++++++++++++++++++++++++++++++++##          ##mm++++++++++++++++++++++++++++++++++++++++++####               \n")
                                                escreva("              ####    ..++++++++++++++++++++++++++++++++++++++##--        ##++++++++++++++++++++++++++++++++++++++++++++@@##++             \n")
                                                escreva("              ####    ++++++++++++++++++++++++++++++++++++++++@@##      ####++++++++++++++++++++++++++++++++++++++++++++@@##mm             \n")
                                                escreva("              ####    ++++++++++++++++++++++++++++++++++++++++++@@########++++++++++++++++++++++++++++++++++++++++++++++@@##mm::           \n")
                                                escreva("              ####  ::++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++####++::           \n")
                                                escreva("              ::####++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++MM####++++           \n")
                                                escreva("                ######++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++######++++::           \n")
